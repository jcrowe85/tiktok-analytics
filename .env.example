# TikTok API Configuration
TIKTOK_CLIENT_KEY=your_client_key_here
TIKTOK_CLIENT_SECRET=your_client_secret_here
TIKTOK_REDIRECT_URI=https://your-domain.com/auth/callback
TIKTOK_ACCESS_TOKEN=your_access_token_here
TIKTOK_REFRESH_TOKEN=your_refresh_token_here

# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password_here
DATABASE_URL=postgresql://tiktok_user:your_secure_postgres_password_here@postgres:5432/tiktok_analytics

# Redis Configuration
REDIS_URL=redis://redis:6379

# AI Service API Keys (Phase 2)
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Application Configuration
NODE_ENV=production
PORT=3000

# Feature Flags (Phase 2)
FEATURE_CREATIVE_INTELLIGENCE=true
FEATURE_AI_ANALYSIS=true

# Processing Configuration (Phase 2)
MAX_CONCURRENT_JOBS=5
JOB_RETRY_ATTEMPTS=3
JOB_TIMEOUT_MS=300000

# Storage Configuration (Phase 2)
ARTIFACTS_STORAGE_PATH=/app/artifacts
ARTIFACTS_MAX_SIZE_MB=100
